<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
{% for categoryName, category in data %}<!-- {{categoryName}} -->
	{% for color in category %}<Color x:Key="SLDS_{{color.name}}">{{color.value}}</Color>
	{% endfor %}
{% endfor %}
</ResourceDictionary>