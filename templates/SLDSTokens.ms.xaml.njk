<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
<!-- Colors -->
{% for categoryName, category in data.Color %}<!-- {{categoryName}} -->
	{% for color in category %}<Color x:Key="SLDS_{{color.name}}">{{color.value}}</Color>
	{% endfor %}
{% endfor %}
<!-- Sizes -->
{% for categoryName, category in data.Size %}<!-- {{categoryName}} -->
	{% for size in category %}<x:Double x:Key="SLDS_{{size.name}}">{{size.value | replace("px", "")}}</x:Double>
	{% endfor %}
{% endfor %}

</ResourceDictionary>